{
  "BASE_DIR":"~/projects/ibuilder_project",
  "board":"artemis_usb2",
  "PROJECT_NAME":"artemis_usb2_default",
  "TEMPLATE":"wishbone_template.json",
  "INTERFACE":{
    "filename":"ft_master_interface.v",
    "bind":{
      "i_ftdi_clk":{
        "loc":"ftdi_clk",
        "direction":"input"
      },
      "io_ftdi_data[7:0]":{
        "loc":"d[7:0]",
        "direction":"inout"
      },
      "i_ftdi_txe_n":{
        "loc":"txe_n",
        "direction":"input"
      },
      "o_ftdi_wr_n":{
        "loc":"wr_n",
        "direction":"output"
      },
      "i_ftdi_rde_n":{
        "loc":"rxe_n",
        "direction":"input"
      },
      "o_ftdi_rd_n":{
        "loc":"rd_n",
        "direction":"output"
      },
      "o_ftdi_oe_n":{
        "loc":"oe_n",
        "direction":"output"
      },
      "o_ftdi_siwu":{
        "loc":"siwua",
        "direction":"output"
      }
    }
  },
  "MEMORY":{
    "ddr3_mem":{
      "filename":"wb_artemis_ddr3.v"
    }
  },
  "SLAVES":{
    "artemis_sata_i":{
      "filename":"wb_artemis_sata_platform.v",
      "constraint_files":[
        "artemis_sata_platform.ucf",
        "artemis_sata_platform_attributes.ucf"
        ],
      "cores":[
        "artemis_sata.ngc"
        ],
      "bind":{
        "i_gtp0_clk_p":{
          "loc":"i_gtp0_clk_p",
          "direction":"input"
        },
        "i_gtp0_clk_n":{
          "loc":"i_gtp0_clk_n",
          "direction":"input"
        },
        "i_sata_phy_rx_p":{
          "loc":"i_sata_phy_rx_p",
          "direction":"input"
        },
        "i_sata_phy_rx_n":{
          "loc":"i_sata_phy_rx_n",
          "direction":"input"
        },
        "i_pcie_phy_rx_p":{
          "loc":"i_pcie_phy_rx_p",
          "direction":"input"
        },
        "i_pcie_phy_rx_n":{
          "loc":"i_pcie_phy_rx_n",
          "direction":"input"
        },
        "o_sata_phy_tx_p":{
          "loc":"o_sata_phy_tx_p",
          "direction":"output"
        },
        "o_sata_phy_tx_n":{
          "loc":"o_sata_phy_tx_n",
          "direction":"output"
        },
        "o_pcie_phy_tx_p":{
          "loc":"o_pcie_phy_tx_p",
          "direction":"output"
        },
        "o_pcie_phy_tx_n":{
          "loc":"o_pcie_phy_tx_n",
          "direction":"output"
        }
      }
    },
    "gpio1":{
      "filename":"wb_gpio.v",
      "bind":{
        "gpio_out[1:0]":{
          "loc":"led[1:0]",
          "direction":"output"
        },
        "gpio_in[3:2]":{
          "loc":"button[1:0]",
          "direction":"input"
        }
      }
    },
    "sata":{
      "filename":"wb_sata.v"
    }
  },
  "internal_bind":{
    "artemis_sata_i_i_sata_tx_char_is_k":{
      "signal":"sata_o_tx_is_k"
    },
    "artemis_sata_i_i_sata_tx_data":{
      "signal":"sata_o_tx_dout"
    },
    "artemis_sata_i_i_sata_tx_elec_idle":{
      "signal":"sata_o_tx_elec_idle"
    },
    "artemis_sata_i_i_sata_tx_comm_init":{
      "signal":"sata_o_tx_comm_reset"
    },
    "artemis_sata_i_i_sata_tx_comm_wake":{
      "signal":"sata_o_tx_comm_wake"
    },
    "sata_i_tx_oob_complete":{
      "signal":"artemis_sata_i_o_sata_tx_oob_complete"
    },

    "sata_i_rx_din":{
      "signal":"artemis_sata_i_o_sata_rx_data"
    },
    "sata_i_rx_is_k":{
      "signal":"artemis_sata_i_o_sata_rx_char_is_k"
    },
    "sata_i_rx_elec_idle":{
      "signal":"artemis_sata_i_o_sata_rx_elec_idle"
    },
    "sata_i_comm_init_detect":{
      "signal":"artemis_sata_i_o_sata_rx_comm_init_detect"
    },
    "sata_i_comm_wake_detect":{
      "signal":"artemis_sata_i_o_sata_rx_comm_wake_detect"
    },
    "sata_i_phy_error":{
      "signal":"artemis_sata_i_o_sata_error"
    },
    "sata_sata_75mhz_clk":{
      "signal":"artemis_sata_i_o_sata_75mhz_clk"
    },
    "sata_i_platform_ready":{
      "signal":"artemis_sata_i_o_platform_ready"
    }
  }
}
