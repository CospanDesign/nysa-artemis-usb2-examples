{
    "BASE_DIR":"~/projects/ibuilder_project",
    "board":"artemis_usb2",
    "PROJECT_NAME":"artemis_usb2_default",
    "TEMPLATE":"wishbone_template.json",
    "INTERFACE":{
        "filename":"ft_master_interface.v",
        "bind":{
            "i_ftdi_clk":{
                "loc":"ftdi_clk",
                "direction":"input"
            },
            "io_ftdi_data[7:0]":{
                "loc":"d[7:0]",
                "direction":"inout"
            },
            "i_ftdi_txe_n":{
                "loc":"txe_n",
                "direction":"input"
            },
            "o_ftdi_wr_n":{
                "loc":"wr_n",
                "direction":"output"
            },
            "i_ftdi_rde_n":{
                "loc":"rxe_n",
                "direction":"input"
            },
            "o_ftdi_rd_n":{
                "loc":"rd_n",
                "direction":"output"
            },
            "o_ftdi_oe_n":{
                "loc":"oe_n",
                "direction":"output"
            },
            "o_ftdi_siwu":{
                "loc":"siwua",
                "direction":"output"
            }
        }
    },
    "MEMORY":{
      "ddr3_mem":{
          "filename":"wb_artemis_ddr3.v"
      }
    },
    "SLAVES":{
        "artemis_usb2_plat":{
            "filename":"wb_artemis_usb2_platform.v",
            "constraint_files":[
             "artemis_pcie_sata_top.ucf",
             "artemis_usb_gtp_attributes.ucf"
            ],
            "cores":[
              "artemis_pcie_sata.ngc"
            ],
            "bind":{
              "i_gtp0_clk_p":{
                "loc":"i_gtp0_clk_p",
                "direction":"input"
              },
              "i_gtp0_clk_n":{
                "loc":"i_gtp0_clk_n",
                "direction":"input"
              },
              "i_gtp1_clk_p":{
                "loc":"i_gtp1_clk_p",
                "direction":"input"
              },
              "i_gtp1_clk_n":{
                "loc":"i_gtp1_clk_n",
                "direction":"input"
              },
              "i_sata_phy_rx_p":{
                "loc":"i_sata_phy_rx_p",
                "direction":"input"
              },
              "i_sata_phy_rx_n":{
                "loc":"i_sata_phy_rx_n",
                "direction":"input"
              },
              "i_pcie_phy_rx_p":{
                "loc":"i_pcie_phy_rx_p",
                "direction":"input"
              },
              "i_pcie_phy_rx_n":{
                "loc":"i_pcie_phy_rx_n",
                "direction":"input"
              },
              "o_sata_phy_tx_p":{
                "loc":"o_sata_phy_tx_p",
                "direction":"output"
              },
              "o_sata_phy_tx_n":{
                "loc":"o_sata_phy_tx_n",
                "direction":"output"
              },
              "o_pcie_phy_tx_p":{
                "loc":"o_pcie_phy_tx_p",
                "direction":"output"
              },
              "o_pcie_phy_tx_n":{
                "loc":"o_pcie_phy_tx_n",
                "direction":"output"
              }
            }
        },
        "gpio1":{
            "filename":"wb_gpio.v",
            "bind":{
                "gpio_out[1:0]":{
                    "loc":"led[1:0]",
                    "direction":"output"
                },
                "gpio_in[3:2]":{
                    "loc":"button[1:0]",
                    "direction":"input"
                }
            }
        },
        "sata":{
          "filename":"wb_sata.v"
        }
    },
    "internal_bind":{
      "artemis_usb2_plat_i_pcie_disparity_mode":{
        "signal":"2'b0"
      },
      "artemis_usb2_plat_i_sata_tx_char_is_k":{
        "signal":"sata_o_tx_isk"
      },
      "artemis_usb2_plat_i_pcie_tx_char_is_k":{
        "signal":"1'b0"
      },
      "artemis_usb2_plat_i_sata_tx_data":{
        "signal":"sata_tx_dout"
      },
      "artemis_usb2_plat_i_pcie_tx_data":{
        "signal":"32'h0000"
      },
      "artemis_usb2_plat_i_pcie_tx_detect_rx":{
        "signal":"1'b0"
      },
      "artemis_usb2_plat_i_sata_tx_elec_idle":{
        "signal":"sata_o_elec_idle"
      },
      "artemis_usb2_plat_i_pcie_tx_elec_idle":{
        "signal":"1'b0"
      },
      "artemis_usb2_plat_i_sata_tx_comm_start":{
        "signal":"o_sata_tx_comm_init"
      },
      "artemis_usb2_plat_i_sata_tx_comm_type":{
        "signal":"1'b0"
      }

   }
}
