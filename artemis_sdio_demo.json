{
  "BASE_DIR":"~/projects/ibuilder_project",
  "board":"artemis_usb2",
  "PROJECT_NAME":"artemis_usb2_default",
  "TEMPLATE":"wishbone_template.json",
  "defines":{
    "DDR3_W0_CH":"",
    "DDR3_R0_CH":""
  },
  "INTERFACE":{
      "filename":"ft_master_interface.v",
      "bind":{
          "i_ftdi_clk":{
              "loc":"ftdi_clk",
              "direction":"input"
          },
          "io_ftdi_data[7:0]":{
              "loc":"d[7:0]",
              "direction":"inout"
          },
          "i_ftdi_txe_n":{
              "loc":"txe_n",
              "direction":"input"
          },
          "o_ftdi_wr_n":{
              "loc":"wr_n",
              "direction":"output"
          },
          "i_ftdi_rde_n":{
              "loc":"rxe_n",
              "direction":"input"
          },
          "o_ftdi_rd_n":{
              "loc":"rd_n",
              "direction":"output"
          },
          "o_ftdi_oe_n":{
              "loc":"oe_n",
              "direction":"output"
          },
          "o_ftdi_siwu":{
              "loc":"siwua",
              "direction":"output"
          }
      }
  },
  "MEMORY":{
    "ddr3_mem":{
      "filename":"wb_artemis_ddr3.v"
    }
  },
  "SLAVES":{
      "gpio1":{
          "filename":"wb_gpio.v",
          "bind":{
              "gpio_out[1:0]":{
                  "loc":"led[1:0]",
                  "direction":"output"
              },
              "gpio_in[3:2]":{
                  "loc":"button[1:0]",
                  "direction":"input"
              }
          }
      },
      "sd_host":{
        "filename":"wb_sd_host.v",
        "bind":{
          "o_sd_clk":{
            "loc":"sd_hst_clk",
            "direction":"output"
          },
          "io_sd_cmd":{
            "loc":"sd_hst_cmd",
            "direction":"inout"
          },
          "io_sd_data":{
            "loc":"sd_hst_data[3:0]",
            "direction":"inout"
          }
        }
      },
      "sdio":{
        "filename":"wb_sdio_device.v",
        "bind":{
          "i_phy_sd_clk":{
            "loc":"sd_clk",
            "direction":"input"
          },
          "io_phy_sd_cmd":{
            "loc":"sd_cmd",
            "direction":"inout"
          },
          "io_phy_sd_data":{
            "loc":"sd_data[3:0]",
            "direction":"inout"
          }
        }
      }
  },
  "dependencies":[
    "sd_sd4_phy.v",
    "sd_host_defines.v",
    "sd_host_stack_defines.v",
    "sdio_fbr.v",
    "sd_dev_platform_spartan6.v",
    "sd_host_platform_spartan6.v",
    "crc16_2bit.v",
    "sdio_cis_rom.rom"
  ],
  "constraint_files":[
    "artemis_sd_host.ucf",
    "artemis_sdio_device.ucf"
  ]
}
